{
  "name": "jbowers-beanstalkd",
  "version": "0.2.1",
  "author": "jbowers",
  "summary": "The beanstalkd module provides an interface for managing a beanstalkd package/service.",
  "license": "Apache-2.0",
  "source": "https://github.com/cubiclelord/beanstalkd",
  "project_page": "https://github.com/cubiclelord/beanstalkd/",
  "issues_url": "https://github.com/cubiclelord/beanstalkd/issues",
  "dependencies": [
    {
      "name":"puppetlabs/stdlib",
      "version_requirement":">=2.0.0 <5.0.0"
    },
    {
      "name":"stahnma/epel",
      "version_requirement":"1.x"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ 
        "12.04", 
        "14.04" 
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6"
      ]
    }
  ]
}
